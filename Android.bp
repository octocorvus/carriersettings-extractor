prebuilt_etc {
    name: "extracted-apns",
    filename: "apns-conf.xml",
    product_specific: true,
    src: ":carrier_extraction-apn",
}

prebuilt_etc {
    name: "extracted-carrierconfig",
    filename: "carrierconfig-vendor.xml",
    product_specific: true,
    src: ":carrier_extraction-cc",
}
