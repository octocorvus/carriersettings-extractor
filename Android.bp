soong_namespace {
}

python_binary_host {
    name: "carriersettings_extractor",
    defaults: ["carrier_extractor_defaults"],
    srcs: [
        "carriersettings_extractor.py",
    ],
    libs: [
        "libprotobuf-python",
        "carriersettings_extractor-proto",
    ],
}

python_defaults {
    name: "carrier_extractor_defaults",
    version: {
        py3: {
            embedded_launcher: false,
        },
    },
    host_supported: true,
}

python_library_host {
    name: "carriersettings_extractor-proto",
    defaults: ["carrier_extractor_defaults"],
    srcs: [
        "*.proto",
    ],
    proto: {
        canonical_path_from_root: false,
    },
}

soong_config_module_type {
    name: "carrier_extractor_genrule",
    module_type: "genrule",
    config_namespace: "carrierconfig-extractor",
    variables: ["product"],
    properties: ["cmd", "out"],
}

soong_config_string_variable {
    name: "product",
    values: [
        "flame",
        "coral",
        "sunfish",
        "bramble",
        "redfin",
        "barbet",
        "oriole",
        "raven",
        "bluejay",
        "panther",
        "cheetah",
    ],
}

carrier_extractor_genrule {
    name: "carrier_extraction-apn",
    tools: ["carriersettings_extractor"],
    soong_config_variables: {
        product: {
            flame: {
                cmd: "$(location carriersettings_extractor) vendor/google_devices/$$TARGET_PRODUCT/proprietary/product/etc/CarrierSettings/ . $(genDir)/apns-conf-flame.xml $(genDir)/carrierconfig-vendor-flame.xml $$TARGET_PRODUCT",
                out: [
                    "apns-conf-flame.xml",
                ],
            },
            coral: {
                cmd: "$(location carriersettings_extractor) vendor/google_devices/$$TARGET_PRODUCT/proprietary/product/etc/CarrierSettings/ . $(genDir)/apns-conf-coral.xml $(genDir)/carrierconfig-vendor-coral.xml $$TARGET_PRODUCT",
